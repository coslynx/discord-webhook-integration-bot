    "body-parser": "^1.20.2",
    "discord.js": "^14.11.0",
    "dotenv": "^10.0.0",
    "express": "^4.18.2",
    "jest": "^29.6.4",
    "mongoose": "^7.0.3",
    "nodemailer": "^6.9.5",
